.homepage_wrapper {
	width:100%;
	margin:0 auto;
	font-family: 'Montserrat';

	.row {
		max-width:100%;
	}

	.columns {
		padding:0px;
	}

	h2 {
		font-weight:700;
		font-family: Montserrat;
	}

	header {
		background:$homepage_background;

		.headerInnerWrap {
		  padding: 0 45px;
		  overflow: hidden;
		  max-height:80px;

		  @media #{$small-only} {
		  	padding: 0 6px;
		  }
		
			.changeCity {
				display: inline-block;
				padding:4px 4px 1px;
				border: 1px solid #FED132;
				border-radius: 2px;
				opacity: 0.6;
				position: relative;
				color:#FED132;
				font-family: Montserrat;
				text-transform: uppercase;
				font-size: 12px;
				vertical-align: top;
				top: 26px;
				margin-left: 10px;

				img {
					width: 15px;
				}

				span {
					position: relative;
					top: -3px;
				}		
			}		

			.changeCity:hover{
			  opacity: 1;
			}

			.siteNameLink {
				display: inline-block;
				border-left:1px solid #aaa;
				margin: 12px 0px 15px -10px;
				padding:10px 0 5px 20px;
				vertical-align: top;
			}

			a.siteNameLink:hover {
				border-bottom-color: transparent;
			}

			.siteNameDesktop {
				margin: 0;
				color: white;
				text-transform: uppercase;
				font-size: 30px;
				line-height: 30px;
				font-weight:700;
			}

			.city-mobile {
				display:none;
			}

			@media #{$small-only} {
				.siteNameDesktop {
					font-size:16px;
				}

				.changeCity {
					display:none;
				}

				.city-mobile {
					display:inline-block;
				}

			}

			.siteNameMobile{
				color:white;
				text-transform: uppercase;
				display: block;
				margin:0;
				text-align: right;
				padding: 20px 20px 18px;
				font-size: 20px;
			}
			.siteNameMobile:before {
				padding: 6px 10px 6px 9px;
				border: 2px solid white;
				display: inline-block;
				width:100px;
				height: 10px;
			}

			.siteLogoDesktop {
				color: white;
				font-family: 'Montserrat';
				text-transform: uppercase;
				font-size:26px;
				font-weight:700;
				display: inline-block;
				max-height: 55px;
				max-width: 110px;
				position: relative;
				top: 5px;
				line-height:32px;

				@media #{$small-only} {
					font-size:20px;
					max-width:90px;
					line-height:28px;
				}

				span {
					display: inline-block;
					line-height: 26px;
					position: relative;
					
					&:nth-child(3) {
						top: -10px;
					}

					&:nth-child(4){
						font-size: 15px;
						line-height: 15px;
						top: -27px;

						@media #{$small-only} {
							font-size:12px;
						}
					}

					i:nth-child(1) {
						position: relative;
						border: 2px solid #FED335;
						background: #FED335;
						width: 7px;
						height: 7px;
						display: inline-block;
						border-radius: 100%;
						bottom: 6px;
						left: 10px;
						-webkit-animation-name: rotate1;
						      animation-name: rotate1;  
						-webkit-animation-duration: 1s;
						      animation-duration: 1s;
						-webkit-animation-fill-mode: both;
						      animation-fill-mode: both;
					}

					i:nth-child(2) {
						position: relative;
						border: 1px solid #FED335;
						width: 15px;
						height: 15px;
						display: inline-block;
						border-radius: 100%;
						z-index: 100;
						bottom: 2px;
						right:1px;
						-webkit-animation-name: rotate1;
						      animation-name: rotate1;
						-webkit-animation-iteration-count: 3;
						      animation-iteration-count: 3;
						-webkit-animation-direction: alternate; /* Chrome, Safari, Opera */
						      animation-direction: alternate;
						-webkit-animation-duration: 1s;
						      animation-duration: 1s;
						-webkit-animation-delay: 0.5s;
						      animation-delay: 0.5s;
						-webkit-animation-fill-mode: both;
						      animation-fill-mode: both;
					}

					i:nth-child(3){
						position: relative;
						border: 1px solid #FED335;
						width: 23px;
						height: 23px;
						display: inline-block;
						border-radius: 100%;
						z-index: 100;
						top: 2px;
						right: 20px;
						-webkit-animation-name: rotate1;
						      animation-name: rotate1;
						-webkit-animation-iteration-count: 3;
						      animation-iteration-count: 3;
						-webkit-animation-direction: alternate; /* Chrome, Safari, Opera */
						      animation-direction: alternate;
						-webkit-animation-duration: 1s;
						      animation-duration: 1s;
						-webkit-animation-delay: 0.75s;
						      animation-delay: 0.75s;
						-webkit-animation-fill-mode: both;
						      animation-fill-mode: both;
					}

					@-webkit-keyframes rotate1 {
						0% {
						opacity: 0;
						-webkit-transform: rotateY(-720deg);
						        transform: rotateY(-720deg);
						}

						100% {
							opacity: 1;
							-webkit-transform: none;
							        transform: none;
						}
					}

					@keyframes rotate1 {
						0% {
						opacity: 0;
						-webkit-transform: rotateY(-720deg);
						        transform: rotateY(-720deg);
						}
						100% {
						opacity: 1;
						-webkit-transform: none;
						        transform: none;
						}
					}

				}
			}

			a.siteLogoDesktop:hover {
			  border-bottom-color: transparent; 
			}

			.nationalApp {
			  display: inline-block;
			  float: right;
			  text-align: center;
			  padding-top: 8px;
			
				span {
					display: block;
					color: white;
					font-family: Montserrat;
					font-size: 12px;
					letter-spacing: 0px;
					margin-bottom: 5px;
				}
				img {
					height: 40px;
				}
				a[href=""]{
					display: none;
				}
			}

			.weather-alerts {
				color:$white;
				text-align:right;
				line-height:1.20rem;
				float:right;
				display:inline-block;
				margin-top:5px;

				.wi {
					font-size:3rem;
					margin-top:15px;
				}

				.weather-icon {
					line-height: 25px;
					vertical-align: text-top;
				}

				.weather-words {
					display: inline-block;
					margin: 0 1em;
					text-align: left!important;
					font-weight: 700;
					position:relative;
					vertical-align:top;
					margin-top:5px;

					.weather-temp {
						font-size:1.65rem;
					}

					.weather-word {
						font-size:1rem;
						color:$social_grey;
						font-weight:700;
					}

					a {
						font-weight:700;
						font-size:.8rem;
						color:$white;
					}
				}
			}

			.traffic-alerts {
				text-align:right;
				color:$white;
				line-height:1.2rem;
				float:right;
				display:inline-block;
				margin-top:5px;

				.traffic-icon {
					max-height: 65px;
					display: inline-block;
					margin-right: 10px;
					vertical-align: top;
				}

				.traffic-words {
					min-height:2em;
					display: inline-block;
					text-align: left!important;
					margin-right: 1em;
					margin-top:5px;

					.traffic-word {
						font-size:1.65rem;
						font-weight:700;
					}

					.traffic-accidents {
						font-size:1rem;
						color:$social_grey;
						font-weight:700;
					}

					a {
						font-size:0.8rem;
						font-weight:700;
						color:$white;
					}
				}
			}
		}

		.top-bar {
			background:$white;

			.menu-icon {
				a {
					color: $black;
				}
			}
		}

		#fixed-nav, .top-bar-section {
			background:$white;
			width:100%;
			height:35px;
			text-align:left;
			position:relative;
			text-transform:uppercase;
			background:$white;

			li:not(.has-form) a:not(.button) {
				padding: 0;
				line-height: 45px;
				background: $white;
			}

			li:not(.has-form) a:not(.button), .homepage_wrapper header .top-bar-section li:not(.has-form) a:not(.button) {
				padding: 0;
				line-height: 41px;
				background: white;

				@media #{$small-only} {
					line-height:10px;
				}
			}


			a {
				line-height:1rem;
				font-size:.675rem;
				font-weight:700;
				color: #0E1213;
				text-decoration: none;
				border-bottom: 4px solid transparent;
				background:$white;
				-webkit-transition: all .4s;
				-moz-transition: all .4s;
				-o-transition: all .4s;
				transition: all .4s;
				padding-bottom:5px;
				text-transform:uppercase;
			}

			a:hover {
				outline: 0;
				border-bottom: 4px solid #FED335;
				background:$white;
			}

			@media #{$medium-down} {
			  	a {
			  		font-size:.6rem;
			  	}
			  }

			ul {
				list-style-type:none;
				margin-bottom:0px;
				margin-left:20px;

				.app-download {
				  color: white;

					a {
						display: inline-block;
						padding:0;
						color: #fff;
						border:0;
					}

					a[href=""] {
						display: none;
					}

					img {
						height: 26px;
						position: relative;
						vertical-align: top;
					}
				}
			}

			li {
				display:inline;
				margin-right:20px;
				font-weight:700;
				text-transform:uppercase;
				background:$white;
			}

			li:hover {
				background:$white;
			}

			.social-sharing {
					color:$black;
					display: inline-block;
					text-align: left;
					margin: 0 0 0 1rem;
					position:absolute;
					top:4px;
					right:55px;

				a {
					width:30px;
					height:30px;
					color:$white;
				}

				span {
					vertical-align:middle;
					position:relative;
				}

				.fa {
					font-size:1rem;
				}

				.fa-twitter {
					right:3px;
				}

				.fa-facebook {
					right:1px;
				}

				.sharer {
					display: inline-block;
					width: 26px;
					height: 26px;
					line-height: 15px;
					padding: 6px 9px;
					font-size: 15px;
					color: #fff;
					border: 0;
				}

				.sharer:hover {
					opacity:.6;
					-webkit-transition: all .4s;
					-moz-transition: all .4s;
					-o-transition: all .4s;
					transition: all .4s;
				}

				.twitter-share {
					background: $twitter_blue;
				}

				.facebook-share {
					background: $facebook_blue;
				}
			}
		}

	}

	section#just-in {
		background: $white;
		max-width:1020px;
		width:100%;
		display: block;
		color: #fff;
		font-family: $open_sans;
		position:relative;
		z-index:2;
		margin: 20px auto 10px;
		padding: 20px;

		@media #{$small-only} {
			.dfp-cube {
				text-align: center;
			}
		}

		.just-in-wrapper {
			position:relative;
			border-bottom:1px solid $homepage_border_grey;
			padding-bottom:15px;

			.large-12 {
				@media #{$medium-up} {
					margin-bottom:20px;
				}

				@media #{$small-only} {
					margin-bottom:0px;
				}
			}

			.headlines-top-story-image {

				@media #{$small-only} {
					text-align: center;
				}

				img {
					max-width:300px;
					margin-top:10px;
				}
			}

			.just-in-alerts {
				line-height:1rem;
				h3 {
					color:$red;
					text-transform:uppercase;
					font-weight:700;
					font-size:.9rem;
					line-height:.4rem;
				}

				a {
					line-height:1rem;
					font-size:.85rem;
					font-weight:700;
					color: #0E1213;
					text-decoration: none;
					border-bottom: 1px solid transparent;
					-webkit-transition: all .4s;
					-moz-transition: all .4s;
					-o-transition: all .4s;
					transition: all .4s;
				}

				a:hover {
					outline: 0;
					border-bottom: 1px solid #FED335;
				}
			}

			.break {
				background: 0 0;
				border: 0;
				border-bottom: 1px solid #eee;
				padding: 0;
				margin-bottom: 20px;
			}
		}

		.headlines {
			list-style-type:none;
			font-family: $open_sans;
			margin-left:22px;
			color:$black;

			@media #{$medium-up} {
				margin-right:45px;
				margin-left: 0px;
			}

			a {
				color:$black;
			}

			.lead {
				margin-bottom: 10px;
				border-bottom: 1px solid $homepage_border_grey;

				h2 {
					font-size:1.5rem;
					font-weight:700;
					line-height:1.5rem;
				}

				p {
					font-size:0.95rem;
					margin-bottom:10px;
					line-height:1.0rem;
				}

				.continue-reading {
					font-weight:700;
				}
			}

			.secondary {
				font-size: 0.875rem;
				font-weight: 700;
				line-height: 1.1rem;
				margin-bottom: 10px;
				border-bottom: 1px solid $homepage_border_grey;
				padding-bottom: 10px;
			}

			.secondary:last-child {
				border-bottom:none;
			}
		}

		.top-cube {
			@media #{$medium-up} {
				padding:20px;
				border-left:1px solid $homepage_border_grey;
			}

			@media #{$small-only} {
				padding:0px;
				border:none;
			}
		}

		.trending {
			margin-top:15px;

			ul {
				list-style-type:none;
				margin:0;
			}

			a {
				line-height:1rem;
				font-size:.85rem;
				font-weight:700;
				color: #0E1213;
				text-decoration: none;
				border-bottom: 1px solid transparent;
				-webkit-transition: all .4s;
				-moz-transition: all .4s;
				-o-transition: all .4s;
				transition: all .4s;
			}

			a:hover {
				outline: 0;
				border-bottom: 1px solid #FED335;
			}

			li {
				display: inline-block;
				min-width:200px;
				max-width: 250px;
				font-weight: 700;
				border-right: 1px solid $homepage_border_grey;
				padding: 0 15px;
				vertical-align: top;
				font-family: $open_sans;
				color:$black;
				overflow:auto;
			}

			li:first-child {
				color:$red;
				border:none;
				text-transform:uppercase;
				padding:0px;
				min-width:150px;
			}

			li:last-child {
				border:none;
			}


		}

	}

	section#section-headlines {
		background: transparent;
		max-width:1040px;
		width:100%;
		display: block;
		color: #fff;
		margin: auto;
		font-family: $open_sans;
		overflow:auto;
		position:relative;
		z-index:2;

		.top-headlines {
			background:$white;
			margin:10px;
			min-height:200px;

			img {
				width:100%;
				height:62px;
				text-align:center;
				margin:0 auto;
				background:$homepage_more_background;
				padding:20px;
			}

			ul {
				list-style-type:none;
				margin-right:1.1rem;
			}

			li {
				display:block;
				border-bottom:1px solid $homepage_border_grey;
				margin-top:10px;
				margin-bottom:10px;
				padding:5px;

				@media #{$large-only} {
					line-height:1rem;
					min-height:95px;
				}

				@media #{$medium-only} {
					line-height:1.25rem;
					min-height:75px;
				}

				@media #{$xmedium-only} {
					line-height:1.25rem;
					min-height:75px;
				}

				@media #{$small-only} {
					line-height:1rem;
					min-height:95px;
				}

				a {
					font-weight:700;
					color: #0E1213;
					text-decoration: none;
					border-bottom: 1px solid transparent;
					-webkit-transition: all .4s;
					-moz-transition: all .4s;
					-o-transition: all .4s;
					transition: all .4s;

					@media #{$large-only} {
						font-size:.85rem;
						line-height:1rem;
					}

					@media #{$medium-only} {
						font-size:1.35rem;
						line-height:1.25rem;
					}

					@media #{$xmedium-only} {
						font-size:1.35rem;
						line-height:1.25rem;
					}

					@media #{$small-only} {
						font-size:1rem;
						line-height:1.25rem;
					}
				}

				a:hover {
					outline: 0;
					border-bottom: 1px solid #FED335;
				}
			}

			li:last-child {
				border-bottom:none;
			}
		}


	}

	section#more-news {
		background: $homepage_more_background;
		max-width:1020px;
		display: block;
		padding: 12px 10px 10px 16px;
		color: $white;
		text-transform: uppercase;
		letter-spacing: 1px;
		margin: 10px auto 10px;
		font-family: $open_sans;
		font-weight:700;
		position:relative;
		z-index:2;
	}

	section#post-cards {
		max-width:1040px;
		display: block;
		color: $black;
		text-transform: uppercase;
		letter-spacing: 1px;
		margin: 20px auto 10px;
		font-family: $open_sans;
		position:relative;
		z-index:2;

		.card-wrap {
			box-shadow: none;
			padding-bottom: 10px;
			height:285px;
			overflow:auto;

			.card {
				margin:10px;
				background: $white;
				padding-bottom:5px;
				overflow:hidden;
				
				p {
					margin-bottom: 0.9rem;
					padding:5px;
				}

				.post-category {
					display: block;
					font-size: .7em;
					font-weight: 700;
					color: $homepage_category_grey;
					margin: 15px 15px 2px;
					text-transform: uppercase;
				}

				.post-title {
					font-weight: 700;
					margin: 2px 15px;
					display: block;
					font-size: 20px;
					line-height: 21px;
					font-family: $open_sans;
					color:$black;
				}

				.post-read-more {
					display:block;
					margin:2px 15px;
					font-weight:400;
					font-size:16px;
					float:right;
					color:$homepage_category_grey;

					.fa {
						color:$social_grey;
					}
				}

				.byline_source {
					font-size:12px;
					color:$social_grey;
					margin-left:15px;
				}

				hr {
					max-width: 90%;
					border: 0;
					border-top: 1px solid #eee;
					margin: 0 auto;
					margin-bottom: 15px;
				}
			}
		}

		.cube {
			overflow:auto;
			width:285px;
			background: $white;
			box-shadow: none;
			padding-bottom: 10px;
		}

	}

	section#traffic {
		background: $white;
		max-width:1020px;
		width:100%;
		margin: 120px auto 40px;
		display: block;
		padding: 20px;
		color: #fff;
		margin: 20px auto 10px;
		position:relative;
		z-index:2;

		.traffic-wrapper {
			position:relative;
			padding-bottom:15px;

			.large-12 {
				margin-right:20px;
				margin-bottom:10px;
			}

			.traffic-border {
				border-right: 1px solid $light_grey;
			}

			h3 {
				font-family: Montserrat;
				line-height: .25rem;
				font-size: 1.25rem;
				color: $homepage_category_grey;
				font-weight:700;
			}

			.traffic-incident {
				margin:0 0 20px 20px;

				.traffic-expected-wrap {
					border-bottom: 1px solid $light_grey;
					display:block;
					width:100%;

					span {
						font-size:.9rem;
						color:$social_grey;
						font-weight:300;
						font-family: sans-serif;
						position:relative;
						display:inline-block;
						background:$white;
						top:8px;
						padding:0 1em 0 0;
					}
				}

				.traffic-map {
					padding:10px;

					img {
						max-width:165px;
					}
				}

				.traffic-details {
					padding:30px 60px 0px 0px;
				}

				.traffic-event {
					font-size:1.15rem;
					color:$black;
					font-weight:700;
					font-family: Montserrat;
					vertical-align: bottom;

					img {
						margin-right:5px;
					}
				}

				.traffic-description {
					font-family: sans-serif;
					font-weight:300;
					color:$black;
					display:block;
					line-height:25px;
				}
